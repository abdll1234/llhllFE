{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/llhllFE/browser",
  "devCommand": "ng serve",
  "installCommand": "npm install",
  "framework": "angular",
  "routes": [
    {
      "src": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
